{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": "off",
    "func-names": "off",
    "object-curly-newline": "off",
    "no-param-reassign": "off",
    "no-shadow": "warn",
    "no-underscore-dangle": "off",
    "padded-blocks": "off",
    "quote-props": "off",
    "semi": "off",
    "space-before-function-paren": "off"
  }
}

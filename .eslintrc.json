{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": "off",
    "padded-blocks": "off",
    "arrow-parens": "off",
    "object-curly-spacing": "off"
  }
}

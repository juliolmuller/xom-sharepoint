{
  "name": "xom-sharepoint",
  "version": "0.1.0",
  "description": "HTTTP client to easily fetch SharePoint through its REST API.",
  "author": "Julio L. Muller",
  "license": "MIT",
  "keywords": [
    "sharepoint",
    "api",
    "rest",
    "restful",
    "fetch",
    "xom"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/xom-sharepoint.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/xom-sharepoint/issues"
  },
  "homepage": "https://github.com/juliolmuller/xom-sharepoint#readme",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:dist && npm run build:node",
    "build:dist": "parcel build src/XomSharePoint.js --no-minify -o xom-sharepoint.js && parcel build src/XomSharePoint.js -o xom-sharepoint.min.js",
    "build:node": "parcel build src/index.js --no-source-maps --no-minify -d ./ --target node",
    "watch": "parcel watch src/index.js",
    "test": "node test/index.js"
  },
  "dependencies": {
    "axios": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.*",
    "@typescript-eslint/parser": "2.*",
    "eslint": "6.*",
    "eslint-config-google": "*",
    "parcel-bundler": "1.*",
    "typescript": "3.*"
  }
}

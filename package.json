{
  "name": "xom-sharepoint",
  "version": "0.13.0",
  "description": "HTTTP client based on Axios to easily fetch SharePoint through its REST API.",
  "author": "Julio L. Muller",
  "license": "MIT",
  "keywords": [
    "sharepoint",
    "api",
    "rest",
    "restful",
    "fetch",
    "xom"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/xom-sharepoint.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/xom-sharepoint/issues"
  },
  "homepage": "https://github.com/juliolmuller/xom-sharepoint#readme",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run build:node && npm run build:dist",
    "build:dist": "parcel build src/dist.js --no-minify -o xom-sharepoint.js && parcel build src/dist.js -o xom-sharepoint.min.js",
    "build:node": "babel src/sharepoint --out-dir build",
    "watch": "parcel watch src/sharepoint/XomSharePoint.js",
    "test": "jest"
  },
  "peerDependencies": {
    "axios": "~0.19.2"
  },
  "dependencies": {
    "@lacussoft/to-arraybuffer": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@lacussoft/eslint-config-standard": "^1.0.4",
    "axios": "~0.19.2",
    "eslint": "^6.8.0",
    "jest": "^25.5.4",
    "parcel-bundler": "^1.12.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": "@lacussoft/standard",
    "rules": {
      "no-underscore-dangle": "off"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
